<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="application.SampleController">
   <center>
      <GridPane alignment="CENTER" hgap="15.0" styleClass="GridPane" stylesheets="@application.css" vgap="15.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <children>
            <Label styleClass="Text" stylesheets="@application.css" text="First Name: " />
            <Label styleClass="Text" stylesheets="@application.css" text="DOB:" GridPane.rowIndex="2" />
            <Label styleClass="Text" stylesheets="@application.css" text="End Date:" GridPane.rowIndex="4" />
            <Label styleClass="Text" stylesheets="@application.css" text="Member Since:" GridPane.rowIndex="3" />
            <Label styleClass="Text" stylesheets="@application.css" text="Last Name:" GridPane.rowIndex="1" />
            <Label styleClass="Text" stylesheets="@application.css" text="Months:" GridPane.rowIndex="5" />
            <Label styleClass="Text" stylesheets="@application.css" text="Monthly Fees:" GridPane.rowIndex="6" />
            <Label styleClass="Text" stylesheets="@application.css" text="Type:" GridPane.rowIndex="7" />
            <Label styleClass="Text" stylesheets="@application.css" GridPane.columnIndex="1" />
            <Label styleClass="Text" stylesheets="@application.css" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label styleClass="Text" stylesheets="@application.css" GridPane.columnIndex="1" GridPane.rowIndex="2" />
            <Label styleClass="Text" stylesheets="@application.css" GridPane.columnIndex="1" GridPane.rowIndex="3" />
            <Label styleClass="Text" stylesheets="@application.css" GridPane.columnIndex="1" GridPane.rowIndex="7" />
            <Label styleClass="Text" stylesheets="@application.css" GridPane.columnIndex="1" GridPane.rowIndex="6" />
            <Label styleClass="Text" stylesheets="@application.css" GridPane.columnIndex="1" GridPane.rowIndex="5" />
            <Label styleClass="Text" stylesheets="@application.css" GridPane.columnIndex="1" GridPane.rowIndex="4" />
         </children>
      </GridPane>
   </center>
   <top>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <TextField />
            <Button mnemonicParsing="false" text="Button" />
         </children>
      </HBox>
   </top>
</BorderPane>
